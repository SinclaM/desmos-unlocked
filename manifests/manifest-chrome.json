{
   "background": {
      "persistent": false,
      "scripts": ["node_modules/webextension-polyfill/dist/browser-polyfill.js", "background.js" ]
   },
   "content_scripts": [ {
      "js": ["node_modules/webextension-polyfill/dist/browser-polyfill.js", "contentscript.js" ],
      "matches": [ "https://www.desmos.com/calculator/*" ],
      "run_at": "document_idle"
   } ],
   "key": "",
   "manifest_version": 2,
   "name": "Desmos Unlocked",
   "page_action": {
      "default_icon": {
         "128": "images/desmos-unlocked-128.png",
         "48" : "images/desmos-unlocked-48.png",
         "32" : "images/desmos-unlocked-32.png",
         "16" : "images/desmos-unlocked-16.png"
      },
      "default_popup": "popup.html"
   },
   "permissions": [ "activeTab", "declarativeContent", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1"
}
